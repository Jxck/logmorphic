  "author": "Jxck",
  "bugs": {
    "url": "https://github.com/Jxck/logmorphic/issues"
  },
  "devDependencies": {
    "eslint": "^2.4.0",
    "eslint-plugin-no-func-space": "0.0.4",
    "eslint-plugin-nodeca": "^1.0.3",
    "typescript": "^1.8.7"
  },
  "directories": {
    "test": "test"
  },
  "homepage": "https://github.com/Jxck/logmorphic",
  "keywords": [
    "isomorphic",
    "localstorage",
    "log"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jxck/logmorphic"
